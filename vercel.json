{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "buildCommand": "prisma generate && next build",
    "installCommand": "npm install",
    "devCommand": "next dev"
  }